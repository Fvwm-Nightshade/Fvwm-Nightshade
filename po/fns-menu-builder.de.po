########################################################################
# Titlebar
########################################################################
msgid "FNS Personal Menu Builder"
msgstr "FNS Persönliche Menüerstellung"

########################################################################
# Root menu
########################################################################

msgid "Root Menu"
msgstr "Hauptmenü"

msgid "Folder"
msgstr "Ordner"

msgid "Application"
msgstr "Programm"

########################################################################
# Personal menu
########################################################################

msgid "Personal Menu"
msgstr "Persönliches Menü"

msgid "Sort entries with drag'n drop."
msgstr "Einträge sortieren mittels drag'n drop."

########################################################################
# Buttons
########################################################################

msgid "Add >"
msgstr "Hinzufügen >"

msgid "Add the selected menu entry."
msgstr "Ausgewählten Menüeintrag hinzufügen."

msgid "< Remove"
msgstr "< Entfernen"

msgid "Remove the selected menu entry.\nAttention:\nRemoving a folder removes all entries below!"
msgstr "Entfernt ausgewählten Menüeintrag.\nAchtung:\nEntfernen eines Ordners entfernt alle darunterliegenden Einträge!"

msgid "You haven't selected a menu entry to remove!"
msgstr "Sie haben keinen Menüeintrag zum entfernen ausgewählt!"

msgid "Create"
msgstr "Erstellen"

msgid "Create a new folder or application entry below of the selected position."
msgstr "Erstellen eines neuen Ordner oder Programmeintrag unterhalb der ausgewählten Stelle."

msgid "Save"
msgstr "Speichern"

msgid "Save the new personal menu."
msgstr "Neues persönliches Menü speichern."

msgid "Quit"
msgstr "Beenden"

msgid "Quit Personal Menu Builder."
msgstr "Persönliche Menüerstellung beenden."

msgid "You have unsaved changes! Continue anyway?"
msgstr "Sie haben ungespeicherte Änderungen! Trotzdem fortfahren?"

########################################################################
# Creation dialog
########################################################################

msgid "Create new application or folder entry"
msgstr "Neuen Programm- oder Ordnereintrag erstellen"

msgid "Click to choose an icon. No change creates no icon."
msgstr "Klicken um ein Icon zu wählen. Keine Auswahl erstellt kein Icon."

msgid "Create a folder entry."
msgstr "Einen Ordnereintrag erstellen."

msgid "Create an application entry."
msgstr "Einen Programmeintrag erstellen."

msgid "Commandstring"
msgstr "Kommandostring"

msgid "Add a command string or choose a path to an executable.\nHint:\n'Exec exec' will be prepended."
msgstr "Kommandostring oder Pfad zu einem ausführbaren Programm einfügen.\nHinweis:\n'Exec exec' wird davor gesetzt."

msgid "Choose an executable."
msgstr "Ausführbares Programm wählen"

msgid "Label"
msgstr "Bezeichnung"

msgid "The label for this entry."
msgstr "Die Bezeichnung dieses Eintrages."

msgid "Create the new entry."
msgstr "Neuen Eintrag erstellen."

msgid "Cancel"
msgstr "Abbrechen"

msgid "Cancel entry creation."
msgstr "Eintragerstellung abbrechen."

########################################################################
# Others
########################################################################
msgid "Warning"
msgstr "Warnung"

msgid "Open File"
msgstr "Datei öffnen"

msgid "Select Folder"
msgstr "Ordner wählen"
