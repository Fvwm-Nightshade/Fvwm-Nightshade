########################################################################
# Titlebar
########################################################################
msgid "Fvwm-Nightshade Base Setup"
msgstr "Fvwm-Nightshade installation de base"

########################################################################
# Environment
########################################################################

msgid " Environment "
msgstr " Environnement "

msgid "Splash On"
msgstr "Activer le splash"

msgid "Run Banner at startup? Default: on"
msgstr "Afficher la bannière au démarrage? Défaut: en marche"

msgid "Path to splash image."
msgstr "Chemin pour l'image splash."

msgid "Set path to splash image."
msgstr "Choix du chemin de l'image splash."

msgid "Composite Manager On"
msgstr "Manager composite en fonction"

msgid "Use composite manager? Default: off."
msgstr "Utilisez le manager composite? Default: stoppé."

msgid "Configure"
msgstr "Configuration"

msgid "Open composite settings."
msgstr "Ouvrir les paramétres composites."

########################################################################
# Desktop
########################################################################

msgid " Desktop "
msgstr " Bureau "

msgid "Wallpaper Dirs:"
msgstr "Rép. papiers peints:"

msgid "Wallpaper directories, separated by ':'."
msgstr "Répertoires des papiers peints, séparés par ':'."

msgid "Icons on Desktop"
msgstr "Icones sur le bureau"

msgid "Use Icons on desktop? Default: off."
msgstr "Utilisez les icones du bureau? par défaut: pas d'icone."

msgid "Icon Size:"
msgstr "Taille d'icône:"

msgid "Change size of the desktop icons."
msgstr "Changer la taille des icones de bureau."

msgid "Desktop scrolling On"
msgstr "Déroulement du bureau en fonction"

msgid "Activate page scrolling when the cursor hits the edge of a page? Default: on."
msgstr "Activation du saut de page quand le curseur touche le coin de la page? par défaut: en marche."

msgid "Desktops:"
msgstr "Bureaux:"

msgid "Set horizontal count of desktop pages."
msgstr "Nombre de pages horizontales du bureau."

msgid "Set vertical count of desktop pages."
msgstr "Nombre de pages verticales du bureau."

########################################################################
# Decorations & Panels
########################################################################

msgid " Decorations & Panels "
msgstr " Décorations & paneaux "

msgid "Used Layout:"
msgstr "Mise en page:"

msgid "The actual FNS layout."
msgstr "Actuelle mise en page FNS."

msgid "Used Theme:"
msgstr "Thème utilisé:"

msgid "The actual FNS decor theme."
msgstr "Le thème actuel FNS."

msgid "Used Font:"
msgstr "Police utilisée:"

msgid "Used default font in FNS."
msgstr "La police utilisée par défaut dans FNS."

########################################################################
# Default Applications
########################################################################

msgid " Default Applications "
msgstr " Applications par défaut "

msgid "Browser:"
msgstr "Browser:"

msgid "Default browser in FNS."
msgstr "Navigateur par défaut de FNS."

msgid "Set path to browser."
msgstr "Indiquez le chemin du navigateur."

msgid "Editor:"
msgstr "Editeur:"

msgid "Default editor in FNS."
msgstr "Editeur par défaut de FNS."

msgid "Set path to editor."
msgstr "Indiquez le chemin de l'éditeur."

msgid "Terminal:"
msgstr "Terminal:"

msgid "Default terminal in FNS."
msgstr "Terminal par défaut de FNS."

msgid "Set path to terminal."
msgstr "Indiquez le chemin du terminal."

msgid "Transset:"
msgstr "Transset:"

msgid "Default transset in FNS."
msgstr "Transset par défaut en FNS."

msgid "Set path to transset."
msgstr "Indiquez le chemin de transet."

