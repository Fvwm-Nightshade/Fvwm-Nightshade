########################################################################
# Titlebar
########################################################################
msgid "FNS Personal Menu Builder"
msgstr "Constructor del Menú Personal FNS"

########################################################################
# Root menu
########################################################################

msgid "Root Menu"
msgstr "Menú Raíz"

msgid "Folder"
msgstr "Carpeta"

msgid "Application"
msgstr "Aplicación"

########################################################################
# Personal menu
########################################################################

msgid "Personal Menu"
msgstr "Menú Personal"

msgid "Sort entries with drag'n drop."
msgstr "Ordenar entradas con arrastar y soltar."

########################################################################
# Buttons
########################################################################

msgid "Add >"
msgstr "Añadir >"

msgid "Add the selected menu entry."
msgstr "Añadir las entradas selecionadas de menú"

msgid "< Remove"
msgstr "< Eliminar"

msgid "Remove the selected menu entry.\nAttention:\nRemoving a folder removes all entries below!"
msgstr "Eliminar la entrada de menú seleccionada.\nAtención:\n¡Borrando una carpeta se eliminan todas las entradas inferiores!"

msgid "You haven't selected a menu entry to remove!"
msgstr "¡No ha seleccionado una entrada de menú para eliminar!"

msgid "Create"
msgstr "Crear"

msgid "Create a new folder or application entry below of the selected position."
msgstr "Crear nueva carpeta o entrada de aplicación inferior de la posición selecionada."

msgid "Save"
msgstr "Guardar"

msgid "Save the new personal menu."
msgstr "Guardar nuevo menú personal."

msgid "Quit"
msgstr "Cerrar"

msgid "Quit Personal Menu Builder."
msgstr "Salir del menú personal de construcción."

msgid "You have unsaved changes! Continue anyway?"
msgstr "¡Tiene cambios no guardados! ¿Continuar de todas formas?"

########################################################################
# Creation dialog
########################################################################

msgid "Create new application or folder entry"
msgstr "Crear nueva entrada de aplicación o carpeta"

msgid "Click to choose an icon."
msgstr "Clicar para escoger un icono."

msgid "Create a folder entry."
msgstr "Crear una entrada de carpeta."

msgid "Create an application entry."
msgstr "Crear una entrada de aplicación."

msgid "Commandstring"
msgstr "Cadena de commandos"

msgid "Add a command string or choose a path to an executable.\nHint:\n'Exec exec' will be prepended."
msgstr "Añadir un comando de caracteres o escoger una localización a un ejecutable.\nNota:\n'Exec exec' se antepondrá."

msgid "Choose an executable."
msgstr "Escoger un ejecutable."

msgid "Label"
msgstr "Etiqueta"

msgid "The label for this entry."
msgstr "La etiqueta para esta entrada."

msgid "Create the new entry."
msgstr "Crear nueva entrada."

msgid "Cancel"
msgstr "Cancelar"

msgid "Cancel entry creation."
msgstr "Cancelar creación de entrada."

########################################################################
# Others
########################################################################
msgid "Warning"
msgstr "Advertencia"

msgid "Open File"
msgstr "Abrir archivo"

msgid "Select Folder"
msgstr "Seleccionar carpeta"
