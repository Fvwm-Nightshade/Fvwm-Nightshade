########################################################################
# Titlebar
########################################################################
msgid "FNS Personal Menu Builder"
msgstr "FNS Menu Builder Personnel"

########################################################################
# Root menu
########################################################################

msgid "Root Menu"
msgstr "Menu principal"

msgid "Folder"
msgstr "Dossier"

msgid "Application"
msgstr "Application"

########################################################################
# Personal menu
########################################################################

msgid "Personal Menu"
msgstr "Menu personnel"

msgid "Sort entries with drag'n drop."
msgstr "Glisser-déposer les entrées pour les classer."

########################################################################
# Buttons
########################################################################

msgid "Add >"
msgstr "Ajouter >"

msgid "Add the selected menu entry."
msgstr "Ajouter l'entrée du menu selectionné."

msgid "< Remove"
msgstr "< Enlever"

msgid "Remove the selected menu entry.\nAttention:\nRemoving a folder removes all entries below!"
msgstr "Effacer l'entrée du menu selectionné.\nAttention:\nLes entrées des sous menus seront aussi effacées!"

msgid "You haven't selected a menu entry to remove!"
msgstr "Vous n'avez seléctionné aucune entrée de menu à effacer!"

msgid "Create"
msgstr "Créer"

msgid "Create a new folder or application entry below of the selected position."
msgstr "Créer une nouvelle entrée de dossier ou une nouvelle entrée d'application en dessous de la position sélectionnée."

msgid "Save"
msgstr "Sauvegarder"

msgid "Save the new personal menu."
msgstr "Enregistrer le nouveau menu personnel."

msgid "Quit"
msgstr "Quitter"

msgid "Quit Personal Menu Builder."
msgstr "Quitter Menu Builder personnel."

msgid "You have unsaved changes! Continue anyway?"
msgstr "Vous n'avez pas sauvegardé les changements! Souhaitez-vous continuer?"

########################################################################
# Creation dialog
########################################################################

msgid "Create new application or folder entry"
msgstr "Créer une nouvelle entrée d'application ou un nouvelle entrée de dossier"

msgid "Click to choose an icon."
msgstr "Choisissez un icône en cliquant dessus."

msgid "Create a folder entry."
msgstr "Créer une entrée de dossier."

msgid "Create an application entry."
msgstr "Créer une entrée d'application."

msgid "Commandstring"
msgstr "Chaîne de commandes"

msgid "Add a command string or choose a path to an executable.\nHint:\n'Exec exec' will be prepended."
msgstr "entrez une chaîne de commandes ou le chemin de l'emplacement de l'executable .\nHint:\n'Exec exec' va être ajouté."

msgid "Choose an executable."
msgstr "Choississez un executable."

msgid "Label"
msgstr "Etiquette"

msgid "The label for this entry."
msgstr "Etiquette pour cette entrée."

msgid "Create the new entry."
msgstr "Créer la nouvelle entrée."

msgid "Cancel"
msgstr "Annuler"

msgid "Cancel entry creation."
msgstr "Annuler la création de l'entrée."

########################################################################
# Others
########################################################################
msgid "Warning"
msgstr "Avertissement"

msgid "Open File"
msgstr "Ouvrir un fichier"

msgid "Select Folder"
msgstr "Selectionner un répertoire"
